import java.util.*;
public class PerfectTrio {
  public static void main(String args[]){  
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int A = sc.nextInt();
      int B = sc.nextInt();
      int C = sc.nextInt();
      int a = B+C;
      int b = A+C;
      int c = A+B;
      if((A == a) || (B == b) || (C == c)){
        System.out.println("YES");  
      } else {
        System.out.println("NO");  
      }
    }
  }
}