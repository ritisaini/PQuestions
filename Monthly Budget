import java.util.*;
public class MonthlyBudget {
   public static void main(String args[]){  
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
    int X = sc.nextInt();
    int Y = sc.nextInt();
    int A = 30*Y;
    if(X >= A){
      System.out.println("YES");  
    } else {
      System.out.println("NO");  
    }
    }
  }
}