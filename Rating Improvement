import java.util.*;
public class RatingImprovement {
  public static void main(String args[]){ 
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      int Z = X + 200;
      if(Y>Z){
        System.out.println("NO");  
      } else if(X>Y){
        System.out.println("NO");  
      } else {
        System.out.println("YES");  
      }
    }
  }
}