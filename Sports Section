import java.util.*;
public class Sports_Section {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int X = sc.nextInt();
        if(X == 8 || X == 9 || X == 10){
            System.out.println("YES");
        } else {
            System.out.println("NO");
        }
    }
}