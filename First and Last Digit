import java.util.*;
public class FirstLastDigit {
  public static void main(String args[]){   
    Scanner sc = new Scanner(System.in);
	  int i=0,sum=0,rem=0,t = sc.nextInt();
	  while(i<t)
	  {
	      int a = sc.nextInt();
	      sum = a%10;
	      while(a>0)
	      {
	          rem = a%10;
	          a = a/10;
	      }
	      sum = sum+rem;
	      System.out.println(sum);
	      i++;
	  }
    }
}