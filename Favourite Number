import java.util.*;
public class FavouriteNum {
   public static void main(String args[]){ 
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int A = sc.nextInt();
      if((A%2==0) && (A%7==0)){
        System.out.println("Alice");  
      } else if((A%2!=0) && (A%9==0)){
        System.out.println("Bob");  
      } else {
        System.out.println("Charlie");  
      }
    }
}
}