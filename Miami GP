import java.util.*;
public class MiamiGP {
  public static void main(String args[]){  
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    while(t-->0){
      int X = sc.nextInt();
      int Y = sc.nextInt();
      if((X*1.07) >= Y){
        System.out.println("YES");      
      } else {
        System.out.println("NO");  
      }
    }
}
}