import java.util.*;
public class CourseRegistration {
   public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
	int t = sc.nextInt();
    while(t-->0){
     int N = sc.nextInt(); 
     int M = sc.nextInt(); 
     int K = sc.nextInt(); 
     int a = M-K;
     int A[] = new int[3];
       if(a >= N){
         System.out.println("YES");  
       } else {
         System.out.println("NO");  
       }
    }
}
}