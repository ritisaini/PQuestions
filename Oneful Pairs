import java.util.*;
public class OnefulPairs {
   public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int X = a + b + (a*b);
    if(X == 111){
      System.out.println("YES");  
    } else {
      System.out.println("NO");  
    }
   } 
}