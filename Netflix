import java.util.*;
public class Netflix {
  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
	int t = sc.nextInt();
	while(t-->0){
	  int A = sc.nextInt(); 
	  int B = sc.nextInt(); 
	  int C = sc.nextInt(); 
	  int X = sc.nextInt(); 
	  int a = A+B;
	  int b = B+C;
	  int c = A+C;
	    if(a >= X || b >= X || c >= X){
	      System.out.println("YES"); 
	    } else {
	      System.out.println("NO");         
	    }
	}
  }
}